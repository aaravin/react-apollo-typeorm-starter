  },
  "homepage": "https://github.com/aaravin/instacraft#readme",
  "engines": {
    "node": "14.3.0"
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
